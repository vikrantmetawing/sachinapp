<html>
<head></head>
<body style="margin-top: 11%;margin-left: 20px">


<div class="row" style="height:2100px; width:900px;padding-bottom:20px">
   <div class="span5" style="float:left;height:2100px; width:900px;">
   
   
		<%= form_tag({ controller:session[:master_user_name], :action => 'adminhome'}, :multipart => true) do %>	
		
		
			<%@t.each do |i|%>
		 <fieldset style="height: 2100px;width:900px; background-color: #F7F2E0; color: #000000; border: 2px solid black;padding-left:20px">
		<br>
		<br>
		<br>
		<br>
		<tab id=t1><center><h1>Edit Home Page Details </h1></center>
		<hr>
		
				
		<br>
		<br>
		 <label class="control-label">
		Company Name
		
		</label>
		<%= text_field_tag(:companyname,i["c_name"], readonly: false ) %><br/>
		
		<label class="control-label">
		Address
		
		</label>
		<%= text_field_tag(:address,i["address"]) %><br/>
		
		<label class="control-label">
