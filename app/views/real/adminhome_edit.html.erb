		<br/>
		
		<p><label class="control-label">
		Slider Image 3
		
		</label>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
		&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
		<%= image_tag(i["slider_3"], :width => 200, :height =>200) %></p>
		
		<p><label for="Upload_image3">Edit Logo</label>
		<%= file_field 'upload3', 'file3' %></p>
	
		
		<br/>
		
		<p><label class="control-label">
		Company Image 1
		
		</label>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
		&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
		<%= image_tag(i["c_img_1"], :width => 200, :height =>200) %></p>
		
		<p><label for="Upload_image4">Edit Logo</label>
		<%= file_field 'upload4', 'file4' %></p>
	
		
		<br/>
		
		<p><label class="control-label">
